{
  "name": "vaultnode-api",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "generate": "prisma generate",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "seed": "ts-node src/seed.ts",
    "lint": "eslint . --ext .ts",
    "openapi:serve": "npx serve -l 3333 ../../openapi"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.0",
    "@fastify/rate-limit": "^10.1.0",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^2.0.1",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "prisma": "^5.20.0",
    "@prisma/client": "^5.20.0",
    "zod": "^3.23.8",
    "uuid": "^9.0.1",
    "node:crypto": "0.1.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
